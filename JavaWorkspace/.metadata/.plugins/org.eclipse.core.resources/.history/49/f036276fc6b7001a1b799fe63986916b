package dp;

public class LCS {
	static int doLCS(String s1, String s2, int m,int n)
	{
		if(m==0 || n==0) return 0;
		
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String s1 = "ABC";
		String s2 = "AC";
		int m = s1.length();
		int n = s2.length();
		System.out.println(doLCS(s1,s2,m,n));
	}

}
