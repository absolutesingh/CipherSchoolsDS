package dp;

public class Fibonacci {
	static int fib(int n)
	{
		if(n==0) return 0;
		if(n==1) return 1;
		int a = fib(n-1);
		int b = fib(n-2);
		int ans = a + b;
		return ans;
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int n = 5;
		System.out.println(fib(n));
	}

}
